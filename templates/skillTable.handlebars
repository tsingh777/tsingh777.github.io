<table class="skillTable table table-responsive">
    <thead class="skillThead">
    <th class="skillth">Beginner</th>
    <th class="skillth">Developed</th>
    <th class="skillth">Experiened</th>
    </thead>

    <tr class="skillTr">
        <td class="skillTd rightBorder">
            {{#each beginners}}
                <div class="skill">
                    <img src="{{this.imgSrc}}" class="skillImg {{this.imgClass}}">
                    <figcaption class="skillCaption">{{this.name}}</figcaption>
                </div>
            {{/each}}
        </td>
        <td class="skillTd rightBorder">
            {{#each developed}}
                <div class="skill">
                    <img src="{{this.imgSrc}}" class="skillImg {{this.imgClass}}">
                    <figcaption class="skillCaption">{{this.name}}</figcaption>
                </div>
            {{/each}}
        </td>
        <td class="skillTd">
            {{#each experienced}}
                <div class="skill">
                    <img src="{{this.imgSrc}}" class="skillImg {{this.imgClass}}">
                    <figcaption class="skillCaption">{{this.name}}</figcaption>
                </div>
            {{/each}}
        </td>
    </tr>


</table>